{
    "name": "laraviet/backpack2fa",
    "description": "Apply Google Authenticator to Laravel Backpack project",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Trung Thanh",
            "email": "thanhcttsp@gmail.com",
            "homepage": "http://portalbeanzvn.com"
        }
    ],
    "homepage": "https://github.com/laraviet/backpack2fa",
    "keywords": ["Laravel", "Backpack2FA"],
    "require": {
        "illuminate/support": "^5",
        "backpack/crud": "^3.5",
        "bacon/bacon-qr-code": "^1.0.3",
        "pragmarx/google2fa-laravel": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Laraviet\\Backpack2FA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laraviet\\Backpack2FA\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraviet\\Backpack2FA\\Backpack2FAServiceProvider"
            ],
            "aliases": {
                "Backpack2FA": "Laraviet\\Backpack2FA\\Facades\\Backpack2FA"
            }
        }
    }
}
